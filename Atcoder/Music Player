#include<bits/stdc++.h>
using namespace std;
int main() {
    int Q;
    cin>>Q;
    int vol=0;
    bool play=false;
    for (int i=0;i<Q;i++) {
        int A;
        cin>>A;
        if (A==1) {
            vol++;
        } else if (A==2) {
            if (vol>=1) {
                vol--;
            }
        } else if (A == 3) {
            play=!play;
        }
        if (play && vol>=3) {
            cout<<"Yes"<<endl;
        } else {
            cout<<"No"<<endl;
        }
    }
