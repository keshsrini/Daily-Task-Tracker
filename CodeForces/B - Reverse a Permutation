#include<bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin>>t;
    while(t--) {
        int n;
        cin>>n;
        vector<int> arr(n);
        for(int i=0;i<n;i++) cin>>arr[i];
        int l=n,c=0;
        for (int i=0;i<n;i++) {
            if (arr[i]!=n-c) {
                l=i;
                break;
            }else c++;
        }
        int r=-1;
        for (int i=l;i<n;i++) {
            if (arr[i]==n-c) {
                r=i;
                break;
            }
        }reverse(arr.begin()+l,arr.begin()+r+1);
        for (int i=0;i<n;i++) {
            cout<<arr[i]<<" ";
        }cout<<endl;
    }
}
